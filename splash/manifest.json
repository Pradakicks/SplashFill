{
    "name": "Splash Fill",
    "version" : "1.0",
    "manifest_version" : 2,
    "permissions": ["storage", "identity"],
    "icons": {
        "48" : "icons/drip.png",
		"128": "icons/drip1.png"
    },
    "background" : {
      "scripts" : ["./background.js"]
    },
    "content_scripts": [
        {
        "matches":["https://www.supremenewyork.com/checkout"],
        "js": ["src/supreme.js"]
        },
        {
			"matches": ["https://*/*/checkouts/*"],
			"js": ["src/shopify.js"]
        }, 
        {
			"matches": [
				"https://*.shopifycs.com/*"
			],
			"js": [
				"src/shopifycc.js"
			],
			"all_frames": true
        },
        {
			"matches": [
				"https://*/products/*"
			],
			"js": ["thirdParty/jquery-3.5.1.min.js","src/shopifyrequest.js"]
		}
    ],
    "browser_action": {
        "default_icon": {                    
            "16": "icons/drip4.png",         
            "24": "icons/drip4.png",         
            "32": "icons/drip4.png"           
          },
        "default_popup":"eyes/login.html",
        "default_title": "Splash"
        
    }
}